#!/usr/bin/env mkb
# Builder mkb file for the bmGameAnalytics extension on iphone
platform IPHONE

options
{
    lib
    output-name="lib/iphone/libbmGameAnalytics.a"
    output-name-debug="lib/iphone/libbmGameAnalytics_d.a"
    cxxflags="-std=c++11"
}
define S3E_BUILD_EXT

subproject {
	bmGameAnalytics_build
}

